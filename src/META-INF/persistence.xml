		<class>br.com.slack.levabreja.model.Mensalidade</class>
		<class>br.com.slack.levabreja.model.Pedido</class>
		<class>br.com.slack.levabreja.model.PedidoProduto</class>
		<class>br.com.slack.levabreja.model.Produto</class>
		<class>br.com.slack.levabreja.model.Setor</class>
		<class>br.com.slack.levabreja.model.UsuarioEmpresa</class>
